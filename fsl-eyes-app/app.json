{
  "checkpointable": false,
  "name": "wallen-{{ app.name }}",
  "executionSystem": "tacc.stampede2.wallen",
  "executionType": "HPC",
  "deploymentPath": "apps/wallen-{{ app.name }}-{{ app.version }}",
  "deploymentSystem": "tacc.work.wallen",
  "helpURI": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki/FSLeyes",
  "label": "{{ app.label }}",
  "shortDescription": "{{ app.description }}",
  "longDescription": "FSLeyes (pronounced fossilise) is the FSL image viewer, released with FSL 5.0.10 onwards.\n\nPlease name the job 'dcvserver' in order to get this to work. Once the job is running, click on Data Files => My Data (Work) and look for the file called FSLeyes_dcvserver.txt for connection instructions.",
  "modules": [
    "load tacc-singularity"
  ],
  "ontology": [],
  "parallelism": "SERIAL",
  "tags": [],
  "templatePath": "runner.sh",
  "testPath": "tester.sh",
  "version": "{{ app.version }}",
  "defaultMaxRunTime": "02:00:00",
  "inputs": [],
  "parameters": [
    {
      "id": "CONTAINER_IMAGE",
      "value": {
        "default": "{{ docker.namespace }}/{{ docker.repo }}:{{ docker.tag }}",
        "type": "string",
        "visible": false,
        "required": true,
        "order": 1000
      }
    }
  ],
  "outputs": []
}
